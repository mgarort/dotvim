snippet verb "verbatim"
\verb|${0:verbatim}|
endsnippet

snippet _ "text underscore"
\textunderscore
endsnippet

snippet ~ "text tilde"
\textasciitilde
endsnippet

snippet ti "text italic"
\textit{${0:italic}}
endsnippet

snippet tb "text bold"
\textbf{${0:bold}}
endsnippet

snippet mbf "mathbf"
\mathbf{${0:mathbold}}
endsnippet

snippet RR "real" 
\R
endsnippet

snippet QQ "Q" 
\Q
endsnippet

snippet ZZ "Z" 
\Z
endsnippet

